void dynasm_translator::op_binary(type t, binop op)
{
    |  pop rdx
    |  pop rax

    switch (t) {
    case type::t_int: {
        switch (op) {
        case binop::op_add:
            |  add eax, edx
            break;
        case binop::op_sub:
            |  sub eax, edx
            break;
        case binop::op_mul:
            |  mul edx
            break;
        case binop::op_div:
            |  mov  ecx, edx
            |  cdq
            |  idiv ecx
            break;
        case binop::op_rem:
            |  mov  ecx, edx
            |  mov  edx, 0
            |  cdq
            |  idiv ecx
            |  mov  eax, edx
            break;
        case binop::op_and:
            |  and eax, edx
            break;
        case binop::op_or:
            |  or eax, edx
            break;
        case binop::op_xor:
            |  xor eax, edx
            break;
        default: assert(0);
        }
        break;
    }
    case type::t_long: {
        switch (op) {
        case binop::op_add:
            |  add rax, rdx
            break;
        case binop::op_sub:
            |  sub rax, rdx
            break;
        case binop::op_mul:
            |  mul rdx
            break;
        case binop::op_div:
            |  mov  rcx, rdx
            |  cdq
            |  idiv rcx
            break;
        case binop::op_rem:
            |  mov  rcx, rdx
            |  mov  rdx, 0
            |  cdq
            |  idiv rcx
            |  mov  rax, rdx
            break;
        case binop::op_and:
            |  and rax, rdx
            break;
        case binop::op_or:
            |  or rax, rdx
            break;
        case binop::op_xor:
            |  xor rax, rdx
            break;
        default: assert(0);
        }
        break;
    }
    default: assert(0);
    }

    |  push rax
}

