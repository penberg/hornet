void dynasm_translator::op_const(type t, int64_t value)
{
    |  push value
}

void dynasm_translator::op_load(type t, uint16_t idx)
{
    |  mov rax, [rbp-8*idx]
    |  push rax
}

void dynasm_translator::op_store(type t, uint16_t idx)
{
    |  pop rax
    |  mov [rbp-8*idx], rax
}

