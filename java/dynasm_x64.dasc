void dynasm_translator::prologue()
{
    |  push rbp
    |  mov rbp, rsp
    |  sub rbp, _method->max_locals*8
}

void dynasm_translator::epilogue()
{
}

void dynasm_translator::begin(std::shared_ptr<basic_block> bblock)
{
